{
  "testRunner": "jest",
  "runnerConfig": "config.json",
  "apps": {
    "ios": {
      "type": "ios.app",
      "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/agrotour.app",
      "build": "xcodebuild -workspace ios/agrotour.xcworkspace -scheme agrotour -configuration Release -sdk iphonesimulator -derivedDataPath ios/build"
    },
    "android": {
      "type": "android.apk",
      "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
      "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release"
    }
  },
  "testSuites": {
    "default": {
      "device": {
        "type": "iPhone14"
      },
      "app": "ios"
    },
    "android": {
      "device": {
        "type": "android.emu.generic.api30"
      },
      "app": "android"
    }
  },
  "configurations": {
    "ios.sim.debug": {
      "device": {
        "type": "iOS",
        "device": {
          "type": "iPhone14"
        }
      },
      "app": "ios"
    },
    "android.emu.debug": {
      "device": {
        "type": "android.emu.generic.api30"
      },
      "app": "android"
    }
  },
  "testRunner": "jest"
}
